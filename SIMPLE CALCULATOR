#include<iostream>
using namespace std;
int main(){
    int a,b;
    cout<<"Enter two input:";
    cin>>a>>b;
    string x;
    cout<<"choose an operation:+,-,*,/"<<endl; 
    cin>>x;
    if(x=="+"){
        cout<<a+b;
    }
    if(x=="-"){
        cout<<a-b;
    }
    if(x=="*"){
        cout<<a*b;
    }
    if(x=="/"){
        cout<<a/b;
    }
}
